<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
  x:Class="RouteGeneratorSample.MainPage"
  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:routeGeneratorSample="clr-namespace:RouteGeneratorSample"
  x:DataType="routeGeneratorSample:MainViewModel">

  <VerticalStackLayout
    Padding="24"
    Spacing="16"
    VerticalOptions="Center"
    HorizontalOptions="Center">

    <Button Text="Audi" Command="{Binding GoToAudiPageCommand}" />
    <Button Text="Volvo" Command="{Binding GoToVolvoPageCommand}" CommandParameter="Julian" />

  </VerticalStackLayout>

</ContentPage>
